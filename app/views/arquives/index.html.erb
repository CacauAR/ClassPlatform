<p id="notice"><%= notice %></p>

<h1>Arquives</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Classe</th>
      <th>Course</th>
      <th>Comment</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @arquives.each do |arquife| %>
      <tr>
        <td><%= arquife.title %></td>
        <td><%= arquife.classe.code %></td>
        <td><%= arquife.course.name %></td>
        <td><%= arquife.comment %></td>
        <td><%= link_to 'Show', arquife %></td>
        <td><%= link_to 'Edit', edit_arquife_path(arquife) %></td>
        <td><%= link_to 'Destroy', arquife, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Arquife', new_arquife_path %>
